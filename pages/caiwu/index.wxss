.photo-wrap {
  position: relative;
  color: #fff;
}

.photo {
  height: 300rpx;
  width: 100%;
  background-size: cover;
}

.mask {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #e32910;
  opacity: 0.4;
}

.title {
  position: absolute;
  z-index: 2;
  top: 0;
  left: 30rpx;
  line-height: 70rpx;
}

.withdraw {
  position: absolute;
  z-index: 2;
  top: 120rpx;
  width: 100%;
  line-height: 70rpx;
  font-size: 50rpx;
  text-align: center;
}

.total {
  position: absolute;
  bottom: 0;
  height: 70rpx;
  line-height: 70rpx;
  text-indent: 40rpx;
  z-index: 2;
  width: 100%;
}

.total-value {
  position: relative;
  z-index: 3;
}

.item {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px #dbdbdb solid;
  padding: 40rpx 20rpx 40rpx 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  align-items: center;
  background-repeat: no-repeat;
  background-size: 40rpx;
  background-position: left center;
  margin-left: 20rpx;  
}

.item.c1 {
  background-image: url(https://www.wendin.cn/public/images/icon/withdraw-depost.png);
}

.item.c2 {
  background-image: url(https://www.wendin.cn/public/images/icon/GRoss-income.png);
}

.item.c3 {
  background-image: url(https://www.wendin.cn/public/images/icon/expenses-and-receipts.png);
}

.item.c4 {
  background-image: url(https://www.wendin.cn/public/images/icon/credit-card.png);
}

.btn-wrap {
  display: flex;
  margin-top: 60rpx;
  justify-content: center;
}

.btn {
  height: 88rpx;
  line-height: 88rpx;
  color: #fff;
  text-align: center;
  width: 700rpx;
  background: #e32910;
  border-radius: 70rpx;
}
