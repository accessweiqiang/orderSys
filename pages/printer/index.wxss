.main{
  width: 100%;
  margin: 20rpx 0;
  text-align: center;
}
.photo{
  width: 200rpx;
  height: 200rpx;
}
.no-data{
  width: 100%;
  font-size: 32rpx;
  text-align: center;
}
.list {
  margin: 70rpx 0;
}
.list .item{
  display: flex;
  width: 100%;
  justify-content: space-between;
  margin-bottom: 30rpx;
}
.list label{
  width: 70%;
  text-indent: 40rpx;
  text-align: left;
}
.list .delete-btn{
  display: inline-block;
  margin-right: 20rpx;
}
.to-add{
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  text-align: center;
  display: block;
  margin: 0 30rpx;
  border-radius: 40rpx;
}